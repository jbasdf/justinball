{
  "name": "justin-ball",
  "description": "Justin Ball's blog",
  "version": "1.0.0",
  "author": "Justin Ball",
  "dependencies": {
    "bulma": "^0.8.0",
    "cheerio": "^1.0.0-rc.3",
    "dangerously-atomic-html": "^1.1.7",
    "gatsby": "^2.19.5",
    "gatsby-image": "^2.2.39",
    "gatsby-link": "^2.2.28",
    "gatsby-paginate": "^1.1.1",
    "gatsby-plugin-google-analytics": "^2.1.34",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-sass": "^2.1.27",
    "gatsby-plugin-sharp": "^2.4.0",
    "gatsby-remark-copy-linked-files": "^2.1.36",
    "gatsby-remark-images": "^3.1.42",
    "gatsby-remark-prismjs": "^3.3.30",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-transformer-remark": "^2.6.48",
    "gatsby-transformer-sharp": "^2.3.13",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.1",
    "prismjs": "^1.21.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-load-script": "0.0.6"
  },
  "keywords": [
    "gatsby",
    "justin ball"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "https://github.com/jbasdf/justinball.git"
  },
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "hot": "gatsby serve",
    "insta": "instaloader jbasdf",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "_format": "prettier --trailing-comma es5 --no-semi --single-quote --write",
    "format": "npm run format:src & npm run format:cms",
    "format:src": "npm run _format 'src/**/*.js'",
    "format:cms": "npm run _format 'cms/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js"
  },
  "devDependencies": {
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "gh-pages": "^2.2.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2"
  }
}
